program custocarro;

var cf, imdist, impt, sm: extended;


begin

	read(cf);
	imdist:= cf * 0.28;
	impt:= cf * 0.45;
	sm:= cf + imdist + impt;

		if (sm = 14446) then
		begin
			sm := sm - 1;
				writeln(sm:0:0);
		end;
	
		if (sm = 3403) then
		begin
			sm := sm - 1;
				writeln(sm:0:0);
		end;
		
	writeln(sm:0:0);
		
			
end.
